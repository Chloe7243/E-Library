{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "routes/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "/routes/admin.py"
    },
    {
      "src": "/users/(.*)",
      "dest": "/routes/users.py"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/routes/auth.py"
    },
    {
      "src": "/(.*)",
      "dest": "/routes/views.py"
    }
  ]
}
